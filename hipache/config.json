{
    "server": {
        "debug": true,
        "accessLog": "/tmp/proxy2_access.log",
        "port": 80,
        "workers": 2,
        "maxSockets": 100,
        "tcpTimeout": 10,
        "deadBackendTTL": 100,
        "deadBackendOn500": true,
        "retryOnError": 3,
        "httpKeepAlive": false,
        "lruCache": {
          "size": 5,
          "ttl": 5
        },
        "https": {
          "port": 443,
          "key": "/certs/server.key",
          "cert": "/certs/devstuff_me_bundle.crt",
          "secureProtocol": "SSLv23_method"
      }
    }
}